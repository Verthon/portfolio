{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "portfolio",
  "description": "Portfolio based on Qwik City boilerplate",
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/static/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write \"src/**/*.{ts,tsx}\"",
    "fmt.check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "preview": "qwik build preview && vite preview --open",
    "scan.deadcode": "knip",
    "start": "vite --open --mode ssr",
    "test.e2e": "playwright test",
    "test.e2e.ui": "playwright test --headed",
    "test.unit": "vitest",
    "test.unit.ci": "vitest run",
    "test.unit.ui": "vitest --ui",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@axe-core/playwright": "4.11.0",
    "@builder.io/qwik": "1.16.0",
    "@builder.io/qwik-city": "1.16.0",
    "@playwright/test": "1.56.1",
    "@types/eslint": "8.44.8",
    "@types/node": "20.12.7",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "@vitest/ui": "2.1.8",
    "eslint": "8.57.0",
    "eslint-plugin-qwik": "1.12.0",
    "knip": "5.42.0",
    "prettier": "3.4.2",
    "typescript": "5.5.4",
    "undici": "7.15.0",
    "vite": "7.1.12",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "3.2.4"
  }
}
